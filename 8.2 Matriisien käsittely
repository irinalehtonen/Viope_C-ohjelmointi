/* Alla on alustettu 5 x 5 kokonaislukutaulukko:

int matriisi[5][5] =
{
4, 6, 25, 88, 5,
34, 5, 300, 4, 65,
78, 43, 11, 90, 125,
98, 585, 12, 63, 21,
45, 35, 9, 5, 1
};

Kopioi alustus ohjelmaan, joka tulostaa taulukon näytölle, sekä laskee taulukon alkioiden summan. Kunkin rivin perään tulostetaan rivinvaihtomerkki. */

#include <stdio.h>

int main()
{
	int matriisi[5][5] =
	{
	4, 6, 25, 88, 5,
	34, 5, 300, 4, 65,
	78, 43, 11, 90, 125,
	98, 585, 12, 63, 21,
	45, 35, 9, 5, 1
	};
	
	printf("Taulukon:\n4 6 25 88 5 \n34 5 300 4 65 \n78 43 11 90 125 \n98 585 12 63 21 \n45 35 9 5 1 \n\n\n");
	
	int summa = 0;

	int x,y,luku,taulukko;

	taulukko = matriisi[5][5];
	
	
	for (x = 0; x < 5; ++x) {
 		for (y = 0; y < 5; ++y) {
		summa = summa + matriisi[y][x];
 }
}

	printf("alkioiden summa on %d\n",summa);

return 0;
}
